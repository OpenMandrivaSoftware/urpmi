msgid "  --sourcerpm    - print sourcerpm.\n"
msgstr "  --sourcerpm    - ime izvornega rpm paketa\n"

#: ../urpmq:83
#, c-format
msgid "  --summary, -S  - print summary.\n"
msgstr "  --summary, -S  - izpiši povzetek.\n"

#: ../urpmq:85
