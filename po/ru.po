msgstr "  --group        - группа\n"

#: ../urpmf:68
msgid "  --name         - package name\n"
msgstr "  --name         - имя пакета\n"

#: ../urpmf:69
msgid "  --obsoletes    - obsoletes tags\n"
msgstr "  --obsoletes    - устаревшие теги\n"

#: ../urpmf:70
