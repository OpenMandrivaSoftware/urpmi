msgid "  --changelog    - print changelog.\n"
msgstr "  --changelog    - prenta breytingasögu.\n"

#: ../urpmq:82
msgid "  --summary, -S  - print summary.\n"
msgstr "  --summary, -S  - prenta samantekt.\n"

