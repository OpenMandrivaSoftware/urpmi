#: ../urpmi.addmedia_.c:39
msgid "  -h             - try to find and use synthesis or hdlist file.\n"
msgstr ""
"  -h             - 嘗試搜尋並使用綜合檔頭清單(synthesis)或 hdlist 檔案。\n"

#: ../urpmi.addmedia_.c:40 ../urpmi.update_.c:63
msgid "  -f             - force generation of hdlist files.\n"
msgstr "  -f             - 強制產生 hdlist 檔案。\n"

#: ../urpmi.addmedia_.c:41 ../urpmi.update_.c:64 ../urpmi_.c:83 ../urpmq_.c:62
msgid "  --wget         - use wget to retrieve distant files.\n"
msgstr "  --wget         - 使用 wget 擷取遠端檔案。\n"

#: ../urpmi.addmedia_.c:42 ../urpmi.update_.c:65 ../urpmi_.c:84 ../urpmq_.c:63
msgid "  --curl         - use curl to retrieve distant files.\n"
msgstr "  --curl         - 使用 curl 擷取遠端檔案。\n"

#: ../urpmi.addmedia_.c:43 ../urpmi.update_.c:66 ../urpmi_.c:85 ../urpmq_.c:64
msgid ""
"  --proxy        - use specified HTTP proxy, the port number is assumed\n"
"                   to be 1080 by default (format is <proxyhost[:port]>).\n"
msgstr ""
"  --proxy        - 使用指定的 HTTP 代理功能，預設使用 port 1080\n"
"                   (格式為 <proxyhost[:port]>)。\n"

#: ../urpmi.addmedia_.c:45 ../urpmi.update_.c:68 ../urpmi_.c:87 ../urpmq_.c:66
msgid ""
"  --proxy-user   - specify user and password to use for proxy\n"
"                   authentication (format is <user:password>).\n"
msgstr ""
"  --proxy-user   - 指定代理服務主機存取的使用者帳號與密碼\n"
"                   一般格式為 <user:passwd>)。\n"

#: ../urpmi.addmedia_.c:47
msgid "  --update       - create an update medium.\n"
msgstr "  --update       - 建立更新的媒介。\n"

#: ../urpmi.addmedia_.c:48
msgid ""
"  --distrib      - automatically create all media from an installation "
"medium.\n"
msgstr "  --distrib      - 自動從安裝媒介建立所有媒介。\n"

#: ../urpmi.addmedia_.c:90
#, c-format
msgid ""
"%s\n"
"no need to give <relative path of hdlist> with --distrib"
msgstr ""
"%s\n"
"--distrib 不需要給予 <檔頭清單 (hdlist) 的相對路徑>"

#: ../urpmi.addmedia_.c:96 ../urpmi.addmedia_.c:113
#, c-format
msgid "unable to update medium \"%s\"\n"
msgstr "無法更新媒介“%s”\n"

#: ../urpmi.addmedia_.c:102
#, c-format
msgid ""
"%s\n"
"<relative path of hdlist> missing\n"
msgstr ""
"%s\n"
"缺少了 <檔頭清單 (hdlist) 的相對路徑>\n"

#: ../urpmi.addmedia_.c:104
#, c-format
msgid ""
"%s\n"
"`with' missing for ftp media\n"
msgstr ""
"%s\n"
"ftp 媒介的 url 缺少了“with”這個關鍵字\n"

#: ../urpmi.addmedia_.c:112
#, c-format
msgid "unable to create medium \"%s\"\n"
msgstr "無法建立媒介“%s”\n"

#: ../urpmi.removemedia_.c:34
msgid ""
"usage: urpmi.removemedia [-a] <name> ...\n"
"where <name> is a medium name to remove.\n"
msgstr ""
"用法：urpmi.removemedia [-a] <媒介名稱> ...\n"
"<媒介名稱> 即需要移除的媒介名稱。\n"

#: ../urpmi.removemedia_.c:36
msgid "  -a             - select all media.\n"
msgstr "  -a             - 選擇全部的媒介。\n"

#: ../urpmi.removemedia_.c:38 ../urpmi.update_.c:70
#, c-format
msgid ""
"\n"
"unknown options '%s'\n"
msgstr ""
"\n"
"不明選項“%s”\n"

#: ../urpmi.removemedia_.c:47
msgid "nothing to remove (use urpmi.addmedia to add a media)\n"
msgstr "無可移除 (使用 urpmi.addmedia 指令新增媒介)\n"

#: ../urpmi.removemedia_.c:49
#, c-format
msgid ""
"the entry to remove is missing\n"
"(one of %s)\n"
msgstr ""
"缺少了需要刪除的項目\n"
"(%s 的其中一個)\n"

#: ../urpmi.update_.c:58
msgid ""
"usage: urpmi.update [options] <name> ...\n"
"where <name> is a medium name to update.\n"
msgstr ""
"用法：urpmi.update [選項] <媒介名稱> ...\n"
"<媒介名稱> 即需要更新的媒介名稱。\n"

#: ../urpmi.update_.c:60
msgid "  -a             - select all non-removable media.\n"
msgstr "  -a             - 選擇所有非移動式媒介。\n"

#: ../urpmi.update_.c:62
msgid ""
"  -d             - force complete computation of depslist.ordered file.\n"
msgstr "  -d             - 強制完成 depslist.ordered 檔案的計算。\n"

#: ../urpmi.update_.c:78
msgid "nothing to update (use urpmi.addmedia to add a media)\n"
msgstr "無可更新 (使用 urpmi.addmedia 指令新增媒介)\n"

#: ../urpmi.update_.c:80
#, c-format
msgid ""
"the entry to update is missing\n"
"(one of %s)\n"
msgstr ""
"缺少了需要更新的項目\n"
"(%s 的其中一個)\n"

#: ../urpmi_.c:63
#, c-format
msgid ""
"urpmi version %s\n"
"Copyright (C) 1999, 2000, 2001, 2002 MandrakeSoft.\n"
"This is free software and may be redistributed under the terms of the GNU "
"GPL.\n"
"\n"
"usage:\n"
msgstr ""
"urpmi 版本 %s\n"
"版權所有 (C) 1999, 2000, 2001, 2002 MandrakeSoft。\n"
"這是自由軟體，遵照 GNU GPL 的條款發佈。\n"
"\n"
"用法：\n"

#: ../urpmi_.c:68
msgid "  --help         - print this help message.\n"
msgstr "  --help         - 顯示此說明訊息。\n"

#: ../urpmi_.c:69 ../urpmq_.c:51
msgid "  --update       - use only update media.\n"
msgstr "  --update       - 只使用更新的媒介。\n"

#: ../urpmi_.c:70 ../urpmq_.c:52
msgid "  --media        - use only the media listed by comma.\n"
msgstr "  --media        - 只使用以逗號列出的媒介。\n"

#: ../urpmi_.c:71 ../urpmq_.c:53
msgid "  --synthesis    - use the synthesis given instead of urpmi db.\n"
msgstr "  --synthesis    - 使用指定的 --synthesis 檔案，而不是 urpmi db。\n"

#: ../urpmi_.c:72
msgid "  --auto         - automatically select a package in choices.\n"
msgstr "  --auto         - 自動選擇適合的套件。\n"

#: ../urpmi_.c:73 ../urpmq_.c:54
msgid ""
"  --auto-select  - automatically select packages to upgrade the system.\n"
msgstr "  --auto-select  - 自動選擇升級系統所需的套件。\n"

#: ../urpmi_.c:74 ../urpmq_.c:55
msgid "  --fuzzy        - impose fuzzy search (same as -y).\n"
msgstr "  --fuzzy        - 加上模糊搜尋(如同 -y)。\n"

#: ../urpmi_.c:75 ../urpmq_.c:57
msgid "  --src          - next package is a source package (same as -s).\n"
msgstr "  --src          - 下個套件是原始碼套件 (如同 -s)。\n"

#: ../urpmi_.c:76
msgid "  --noclean      - keep rpm not used in cache.\n"
msgstr "  --noclean      - 保留不刪除快取目錄內未使用的 rpm 檔案。\n"

#: ../urpmi_.c:77 ../urpmq_.c:61
msgid ""
"  --force        - force invocation even if some packages do not exist.\n"
msgstr "  --force        - 強制執行安裝即使某些套件不存在。\n"

#: ../urpmi_.c:78
msgid ""
"  --allow-nodeps - allow asking user to install packages without\n"
"                   dependencies checking.\n"
msgstr "  --allow-nodeps - 允許略過相依性的檢測直接安裝上套件。\n"

#: ../urpmi_.c:80
msgid ""
"  --allow-force  - allow asking user to install packages without\n"
"                   dependencies checking and integrity.\n"
msgstr "  --allow-force  - 允許強制暴力安裝上套件而不檢視相衝突等資訊。\n"

#: ../urpmi_.c:82
msgid "  --parallel     - distributed urpmi accross machines of alias.\n"
msgstr ""

#: ../urpmi_.c:89
msgid ""
"  --bug          - output a bug report in directory indicated by next arg.\n"
msgstr ""

#: ../urpmi_.c:90
msgid "  --env          - use specific environment (typically a bug report).\n"
msgstr "  --env          - 使用特定環境 (一般用於臭蟲回報)。\n"

#: ../urpmi_.c:91
msgid "  --X            - use X interface.\n"
msgstr "  --X            - 使用 X 界面。\n"

#: ../urpmi_.c:92
msgid ""
"  --best-output  - choose best interface according to the environment:\n"
"                   X or text mode.\n"
msgstr ""
"  --best-output  - 按照目前環境選擇最佳的界面：\n"
"                   X 或文字模式。\n"

#: ../urpmi_.c:94
msgid "  --verify-rpm   - verify rpm signature before installation.\n"
msgstr "  --verify-rpm   - 安裝套件前確認簽名資訊是否正確。\n"

#: ../urpmi_.c:95
msgid "  -a             - select all matches on command line.\n"
msgstr "  -a             - 選擇命令列中所有符合項目。\n"

#: ../urpmi_.c:96
msgid "  -p             - allow search in provides to find package.\n"
msgstr "  -p             - 允許搜尋套件與程式庫清單(provides)以找到套件。\n"

#: ../urpmi_.c:97 ../urpmq_.c:45
msgid "  -P             - do not search in provides to find package.\n"
msgstr "  -P             - 不為了找到套件而搜尋套件與程式庫清單(provides)。\n"

#: ../urpmi_.c:98 ../urpmq_.c:46
msgid "  -y             - impose fuzzy search (same as --fuzzy).\n"
msgstr "  -y             - 加上模糊搜尋( 如同 --fuzzy)。\n"

#: ../urpmi_.c:99 ../urpmq_.c:47
msgid "  -s             - next package is a source package (same as --src).\n"
msgstr "  -s             - 下個套件是原始檔套件 (如同 --src)。\n"

#: ../urpmi_.c:100
msgid "  -q             - quiet mode.\n"
msgstr "  -q             - 無資訊顯示模式。\n"

#: ../urpmi_.c:101 ../urpmq_.c:41
msgid "  -v             - verbose mode.\n"
msgstr "  -v             - 顯示資訊模式。\n"

#: ../urpmi_.c:102
msgid "  names or rpm files given on command line are installed.\n"
msgstr " 於命令列指定的名稱或 rpm 檔案已經安裝。\n"

#: ../urpmi_.c:166
#, c-format
msgid "urpmi: unknown option \"-%s\", check usage with --help\n"
msgstr "urpmi：不詳的選項“-%s”，請輸入 --help 選項閱讀使用方法\n"

#: ../urpmi_.c:191
#, c-format
msgid "Unable to create directory [%s] for bug report"
msgstr "無法建立 [%s] 目錄，無法啟動臭蟲回報功能"

#: ../urpmi_.c:216
msgid "Only superuser is allowed to install packages"
msgstr "抱歉，只有超級使用者才允許安裝套件"

#: ../urpmi_.c:221 ../urpmi_.c:500 ../urpmi_.c:510 ../urpmi_.c:517
#: ../urpmi_.c:530 ../urpmi_.c:537
msgid "Installation failed"
msgstr "安裝失敗"

#: ../urpmi_.c:314
#, c-format
msgid "One of the following packages is needed to install %s:"
msgstr "須要安裝下列套件 %s："

#: ../urpmi_.c:315
msgid "One of the following packages is needed:"
msgstr "需要以下其中一個套件："

#: ../urpmi_.c:323
#, c-format
msgid "What is your choice? (1-%d) "
msgstr "那一個是你的選擇﹖ (1-%d) "

#: ../urpmi_.c:326
msgid "Sorry, bad choice, try again\n"
msgstr "對不起，選擇錯誤，請再試一次\n"

#: ../urpmi_.c:345
#, c-format
msgid ""
"Some package requested cannot be installed:\n"
"%s\n"
"do you agree ?"
msgstr ""
"下列為無法正確安裝上所需要的套件項目：\n"
"%s\n"
"您同意要繼續嗎？"

#: ../urpmi_.c:362
#, c-format
msgid ""
"The following packages have to be removed for others to be upgraded:\n"
"%s\n"
"do you agree ?"
msgstr ""
"為了可以順利升級，下列套件必須先行移除：\n"
"%s\n"
"您同意要繼續嗎？"

#: ../urpmi_.c:400 ../urpmi_.c:409
#, c-format
msgid ""
"To satisfy dependencies, the following packages are going to be installed (%"
"d MB)"
msgstr "為了使用選取了的套件能正常執行，以下的套件亦需要一同安裝。 (%d MB)"

#: ../urpmi_.c:406
#, c-format
msgid ""
"You need to be root to install the following dependencies:\n"
"%s\n"
msgstr ""
"您需要使用 root 權限執行該程式以便於具備安裝下列相依性項目：\n"
"%s\n"

#: ../urpmi_.c:427 ../urpmq_.c:200
msgid "unable to get source packages, aborting"
msgstr "無法取得原始碼套件，放棄"

#: ../urpmi_.c:438
#, c-format
msgid "Please insert the medium named \"%s\" on device [%s]"
msgstr "請將具有名稱“%s”的媒介放入到裝置 [%s] 內"

#: ../urpmi_.c:439
msgid "Press Enter when ready..."
msgstr "完成後請按 <Enter鍵>．．"

#: ../urpmi_.c:461
msgid "The following packages have bad signatures"
msgstr "下列套件有不合法的簽名檔案資訊"

#: ../urpmi_.c:462
msgid "Do you want to continue installation ?"
msgstr "您要繼續進行安裝嗎？"

#: ../urpmi_.c:485
msgid ""
"Installation failed, some files are missing.\n"
"You may want to update your urpmi database"
msgstr "安裝失敗，因為找不到某些檔案。您可能要更新您的 urpmi 資料庫。"

#: ../urpmi_.c:490
#, c-format
msgid "distributing %s\n"
msgstr "正在發佈 %s \n"

#: ../urpmi_.c:524
msgid "Try installation without checking dependencies? (y/N) "
msgstr "略過檢查套件的相依性檢查強制安裝？(y/N)(是/否)"

#: ../urpmi_.c:532
msgid "Try installation even more strongly (--force)? (y/N) "
msgstr "使用強制性的安裝方式 (--force)？(y/N)(是/否)"

#: ../urpmi_.c:544
msgid "everything already installed"
msgstr "全部都已安裝"

#: ../urpmq_.c:35
#, c-format
msgid ""
"urpmq version %s\n"
"Copyright (C) 2000, 2001, 2002 MandrakeSoft.\n"
"This is free software and may be redistributed under the terms of the GNU "
"GPL.\n"
"\n"
"usage:\n"
msgstr ""
"urpmq 版本 %s\n"
"版權所有 (C) 2000, 2001, 2002 MandrakeSoft。\n"
"這是自由軟體，遵照 GNU GPL 條款發佈。\n"
"\n"
"用法：\n"

#: ../urpmq_.c:40
msgid "  -h             - print this help message.\n"
msgstr "  -h             - 顯示此說明訊息。\n"

#: ../urpmq_.c:42
msgid "  -d             - extend query to package dependencies.\n"
msgstr "  -d             - 查詢其他相關套件。\n"

#: ../urpmq_.c:43
msgid ""
"  -u             - remove package if a more recent version is already "
"installed.\n"
msgstr "  -u             - 如果已安裝較好版本則移除套件。\n"

#: ../urpmq_.c:44
msgid ""
"  -c             - choose complete method for resolving requires closure.\n"
msgstr "  -c             - 選擇使用完整方法去解決與要求最接近的處理。\n"

#: ../urpmq_.c:48
msgid "  -g             - print groups with name also.\n"
msgstr "  -g             - 顯示群組及套件名稱。\n"

#: ../urpmq_.c:49
msgid "  -r             - print version and release with name also.\n"
msgstr "  -r             - 顯示版本、發行資訊與套件名稱\n"

#: ../urpmq_.c:50
msgid "  -f             - print version, release and arch with name.\n"
msgstr "  -f             - 顯示版本，包含套件名稱的發行資訊以及平台版本。\n"

#: ../urpmq_.c:56
msgid "  --list         - list available packages.\n"
msgstr "  --all          - 顯示可用的的套件項目。\n"

#: ../urpmq_.c:58
msgid ""
"  --headers      - extract headers for package listed from urpmi db to\n"
"                   stdout (root only).\n"
msgstr ""
"  --headers      - 從 urpmi 資料庫中找出套件的檔頭，顯示於\n"
"                   標準輸出中 (只限 root 帳號)。\n"

#: ../urpmq_.c:60
msgid ""
"  --sources      - give all source packages before downloading (root only).\n"
msgstr "  --sources      - 下載前先給予所有原始碼套件(root 專用)。\n"

#: ../urpmq_.c:68
msgid "  names or rpm files given on command line are queried.\n"
msgstr "  在命令列給予的名稱或 rpm 檔案都會被查詢。\n"

#: ../urpmq_.c:124
#, c-format
msgid "urpmq: unknown option \"-%s\", check usage with --help\n"
msgstr "urpmq：不認識的選項“-%s”，請輸入 --help 選項閱讀使用方法\n"

#: ../urpmq_.c:127
#, c-format
msgid "urpmq: cannot read rpm file \"%s\"\n"
msgstr "urpmq：無法讀取套件檔“%s”\n"

