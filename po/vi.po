msgid "  --all           - print all tags."
msgstr "  --all           - in ra toàn bộ tag."

#: placeholder.h:25
msgid ""
"  --name          - print tag name: rpm filename (assumed if no tag given on"
msgstr ""
"  --name          - in tên tag: tên tập tin rpm (giả sử không đưa tag vào"

