msgid "Ok"
msgstr "Гаразд"

#: ../gurpmi2:110
msgid "Warning"
msgstr "Попередження"

#: ../gurpmi2:146
msgid " (to upgrade)"
msgstr " (поновити)"

#: ../gurpmi2:147
msgid " (to install)"
msgstr " (встановити)"

