#: ../urpmi:62
#, c-format
msgid "no package named %s\n"
msgstr "沒有套件名稱 %s\n"

#: ../urpmi:63
#, c-format
msgid "The following packages contain %s: %s\n"
msgstr "這套件包含了 %s: %s\n"

#: ../urpmi:70
msgid "rpm database query failed\n"
msgstr "rpm資料庫查尋失敗\n"

#: ../urpmi:79
msgid "The following packages are going to be installed"
msgstr "這套件已被安裝"

